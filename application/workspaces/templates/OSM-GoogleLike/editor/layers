				EXPRESSION: ("[bridge]"="1" and ("[type]" = "unclassified" or "[type]" = "residential" or "[type]" = "service" or "[type]" = "road" or "[type]" = "living_street"))
                STYLE {
					WIDTH: @other_width
                    OUTLINECOLOR: 136 136 136
                    OUTLINEWIDTH: 1
                    LINECAP: BUTT
                    COLOR: @other_clr
				}
			}
		}
    	CLASS {
			15-17 {
				EXPRESSION: ("[bridge]"="1" and "[type]" = "pedestrian")
