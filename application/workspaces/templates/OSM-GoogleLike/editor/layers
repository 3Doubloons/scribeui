                    OUTLINEWIDTH: 1
                    LINECAP: BUTT
                    COLOR: @primary_clr
				}
			}
		}
    	CLASS {
			15-17 {
				EXPRESSION: ("[bridge]"="1" and ("[type]" = "secondary" or "[type]" = "secondary_link"))
                STYLE {
					WIDTH: @secondary_width
                    OUTLINECOLOR: 136 136 136
                    OUTLINEWIDTH: 1
                    LINECAP: BUTT
                    COLOR: @secondary_clr
				}
			}
		}
    	CLASS {
