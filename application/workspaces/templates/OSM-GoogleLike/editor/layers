			1-16: "geometry from (select geometry, osm_id, type, name from osm_new_transport_points where type in ('station', 'subway_entrance', 'terminal', 'tram_stop')) as foo using unique osm_id using srid=900913"
			17: "geometry from (select geometry, osm_id, type, name from osm_new_transport_points where type in ('bus_stop', 'station', 'subway_entrance', 'terminal', 'tram_stop')) as foo using unique osm_id using srid=900913"
		}
    	CLASS {
			EXPRESSION: 'bus_stop'
            TEXT: ' '
            LABEL {
				@labelconfig_public_transport
        		STYLE {
					COLOR: 255 255 255
