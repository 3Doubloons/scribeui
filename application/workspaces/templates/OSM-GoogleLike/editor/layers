






LAYERS {
LAYER {
	1-17 {
        NAME: 'land'
        TYPE: POLYGON
        GROUP: 'default'
        STATUS: ON
        PROJECTION {
            1-3 {{
				'init=epsg:4326'
            }}
        	4-17 {{
				'init=epsg:900913'
			}}
        }
        PROCESSING: 'LABEL_NO_CLIP=ON'
        PROCESSING: 'CLOSE_CONNECTION=DEFER'
		DATA {
			1-3: "TM_WORLD_BORDERS-0.3.shp"
            4-7: "shoreline_300"
            8-17: "processed_p"
		}
        CLASS {
            STYLE {
            	COLOR: '#F4F3F0'
    			OUTLINECOLOR: '#F4F3F0'
            }
        }
    }
}

LAYER {
	2-17 {
        NAME: 'landusage'
        TYPE: POLYGON
        @layerconfig
        @connection
		DATA {
			1-3: "geometry from (select geometry ,osm_id, type, name from osm_new_landusages_gen0 where type in ('forest','residential') order by area desc) as foo using unique osm_id using srid=900913"
            4-9: "geometry from (select geometry ,osm_id, type, name from osm_new_landusages_gen0 where type in ('forest','industrial','commercial','residential','park', 'nature_reserve') order by area desc) as foo using unique osm_id using srid=900913"
            10-12: "geometry from (select geometry ,osm_id, type, name from osm_new_landusages_gen1 where type in ('forest','pedestrian','cemetery','industrial','commercial','brownfield','residential','school','college','university','military','park','golf_course','hospital','parking','stadium','sports_center','pitch', 'nature_reserve') order by area desc) as foo using unique osm_id using srid=900913"
			13-17: "geometry from (select geometry ,osm_id, type, name from osm_new_landusages where type in ('forest','pedestrian','cemetery','industrial','commercial','brownfield','residential','school','college','university','military','park','golf_course','hospital','parking','stadium','sports_center','pitch', 'nature_reserve') order by area desc) as foo using unique osm_id using srid=900913"
