                        PRIORITY: @VAR.roads_lbl_priority
 						SIZE: @VAR.other_lbl_size
                    	COLOR: @VAR.other_lbl_clr
                    	OUTLINECOLOR: @VAR.other_clr
					]	
				]
        	]
		]
    	CLASS [
            13-17 [
				EXPRESSION: 'track'
            	TEXT: ("[name]")
            	STYLE [
            		1-17 [
                   		OUTLINEWIDTH: @VAR.track_ol_width
                    	OUTLINECOLOR: @VAR.track_ol_clr
